{
    "name": "Fiverr Notifier",
    "description": "It is for making notifications on fiverr.com.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "Icons/icon16.png",
        "32": "Icons/icon32.png",
        "48": "Icons/icon48.png",
        "128": "Icons/icon128.png"
    },
    "content_scripts": [{
        "matches": ["*://www.fiverr.com/*"],
        "js": ["JS/content.min.js"]
    }]
}